{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "node_modules/**"
        ]
      }
    }
  ],
  "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        }
    ],
    "functions": {
    "api/index.ts": {
      "includeFiles": [
        "dist/**"
      ],
      "excludeFiles": [
        "node_modules/.prisma/**",
        "node_modules/typescript/**",
        "node_modules/@nestjs/cli/**",
        "**/*.spec.ts",
        "**/*.test.ts",
        "**/__tests__/**"
      ]
    }
  }
}
